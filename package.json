{
  "name": "renotion",
  "version": "1.1.1",
  "description": "A Notion style viewer and editor for markdown",
  "keywords": [
    "devable",
    "notion",
    "markdown",
    "editor",
    "viewer"
  ],
  "homepage": "https://github.com/itswagi/renotion",
  "bugs": {
    "url": "https://github.com/itswagi/renotion/issues"
  },
  "author": "Wajahat Zia <waji.zc@gmail.om> (https://devable.dev)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itswagi/renotion.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "concurrently": "^9.2.1",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "build": "tsup",
    "ci": "npm run build && npm run check-format && npm run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "test": "vitest run",
    "dev": "concurrently \"tsup --watch\" \"tsc --noEmit --watch\"",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci",
    "type-check": "tsc --noEmit",
    "check": "npm run build && npm run type-check && npm run test"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "clsx": "^2.1.1",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.542.0",
    "prism-react-renderer": "^2.4.1"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  }
}
